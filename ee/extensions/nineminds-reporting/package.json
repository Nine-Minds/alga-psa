{
  "name": "@alga/nineminds-reporting",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Nine Minds Reporting Extension - Fetch and display reporting data",
  "scripts": {
    "clean": "rm -rf dist ui/dist",
    "build": "alga build",
    "build:ui": "vite build -c vite.iframe.config.ts",
    "build:all": "npm run build && npm run build:ui",
    "pack": "alga pack",
    "dev": "vite",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@alga-psa/extension-runtime": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@alga-psa/cli": "^0.2.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
