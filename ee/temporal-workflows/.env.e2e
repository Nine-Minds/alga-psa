# E2E Test environment configuration
NODE_ENV=test

# Main application database (for activities testing)
DB_HOST=localhost
DB_PORT=5432
DB_USER_ADMIN=postgres
DB_PASSWORD_ADMIN=postpass123
DB_NAME_SERVER=server
DB_SSL=false

# Temporal configuration for E2E tests
TEMPORAL_ADDRESS=localhost:7233
TEMPORAL_NAMESPACE=default
TEMPORAL_TASK_QUEUE=e2e-temporal-workflows

# Test worker configuration
WORKER_CONCURRENCY=1
WORKER_MAX_ACTIVITIES_PER_SECOND=10
WORKER_MAX_WORKFLOWS_PER_SECOND=10