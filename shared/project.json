{
  "name": "@alga-psa/shared",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "shared/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "options": {
        "script": "build"
      },
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    },
    "lint": {
      "executor": "nx:run-script",
      "options": {
        "script": "lint"
      },
      "cache": true
    }
  },
  "tags": ["scope:shared", "type:library"]
}
