{
  "name": "@alga-psa/event-bus",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./events": {
      "types": "./src/events.ts",
      "import": "./dist/events.mjs",
      "require": "./dist/events.js"
    },
    "./publishers": {
      "types": "./src/publishers/index.ts",
      "import": "./dist/publishers/index.mjs",
      "require": "./dist/publishers/index.js"
    },
    "./publishers/*": {
      "types": "./src/publishers/*.ts",
      "import": "./dist/publishers/*.mjs",
      "require": "./dist/publishers/*.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@alga-psa/core": "*",
    "@alga-psa/event-schemas": "*",
    "@alga-psa/types": "*",
    "redis": "^4.7.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.7.3"
  }
}
