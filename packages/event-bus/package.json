{
  "name": "@alga-psa/event-bus",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./events": {
      "types": "./dist/events.d.ts",
      "import": "./dist/events.js",
      "require": "./dist/events.cjs"
    },
    "./publishers": {
      "types": "./dist/publishers/index.d.ts",
      "import": "./dist/publishers/index.js",
      "require": "./dist/publishers/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@alga-psa/core": "*",
    "@alga-psa/event-schemas": "*",
    "@alga-psa/types": "*",
    "redis": "^4.7.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.7.3"
  }
}
