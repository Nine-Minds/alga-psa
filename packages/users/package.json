{
  "name": "@alga-psa/users",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./actions": {
      "import": "./src/actions/index.ts",
      "types": "./src/actions/index.ts"
    },
    "./actions/*": {
      "import": "./src/actions/*.ts",
      "types": "./src/actions/*.ts"
    },
    "./hooks": {
      "import": "./src/hooks/index.ts",
      "types": "./src/hooks/index.ts"
    },
    "./hooks/*": {
      "import": "./src/hooks/*.ts",
      "types": "./src/hooks/*.ts"
    },
    "./components": {
      "import": "./src/components/index.ts",
      "types": "./src/components/index.ts"
    },
    "./components/*": {
      "import": "./src/components/*.tsx",
      "types": "./src/components/*.tsx"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@alga-psa/auth": "*",
    "@alga-psa/core": "*",
    "@alga-psa/db": "*",
    "@alga-psa/types": "*",
    "@alga-psa/validation": "*",
    "@alga-psa/ui": "*",
    "swr": "^2.3.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}
