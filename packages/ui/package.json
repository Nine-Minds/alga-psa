{
  "name": "@alga-psa/ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components": {
      "types": "./src/components/index.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    },
    "./components/*": {
      "types": "./src/components/*.tsx",
      "import": "./dist/components/*.mjs",
      "require": "./dist/components/*.js"
    },
    "./ui-reflection": {
      "types": "./src/ui-reflection/index.ts",
      "import": "./dist/ui-reflection/index.mjs",
      "require": "./dist/ui-reflection/index.js"
    },
    "./ui-reflection/*": {
      "types": "./src/ui-reflection/*.tsx",
      "import": "./dist/ui-reflection/*.mjs",
      "require": "./dist/ui-reflection/*.js"
    },
    "./lib": {
      "types": "./src/lib/index.ts",
      "import": "./dist/lib/index.mjs",
      "require": "./dist/lib/index.js"
    },
    "./lib/*": {
      "types": "./src/lib/*.ts",
      "import": "./dist/lib/*.mjs",
      "require": "./dist/lib/*.js"
    },
    "./lib/i18n/config": {
      "types": "./src/lib/i18n/config.ts",
      "import": "./dist/lib/i18n/config.mjs",
      "require": "./dist/lib/i18n/config.js"
    },
    "./lib/i18n/client": {
      "types": "./src/lib/i18n/client.tsx",
      "import": "./dist/lib/i18n/client.mjs",
      "require": "./dist/lib/i18n/client.js"
    },
    "./lib/i18n/*": {
      "types": "./src/lib/i18n/*.tsx",
      "import": "./dist/lib/i18n/*.mjs",
      "require": "./dist/lib/i18n/*.js"
    },
    "./hooks": {
      "types": "./src/hooks/index.ts",
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.js"
    },
    "./hooks/*": {
      "types": "./src/hooks/*.ts",
      "import": "./dist/hooks/*.mjs",
      "require": "./dist/hooks/*.js"
    },
    "./editor": {
      "types": "./src/editor/index.ts",
      "import": "./dist/editor/index.mjs",
      "require": "./dist/editor/index.js"
    },
    "./editor/*": {
      "types": "./src/editor/*.tsx",
      "import": "./dist/editor/*.mjs",
      "require": "./dist/editor/*.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@alga-psa/db": "*",
    "@alga-psa/types": "*",
    "@blocknote/core": "^0.22.0",
    "@blocknote/react": "^0.22.0",
    "@blocknote/mantine": "^0.22.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@radix-ui/react-visually-hidden": "^1.1.0",
    "@radix-ui/themes": "^3.1.3",
    "@rjsf/utils": "^6.1.2",
    "@tanstack/react-table": "^8.20.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "cookies-next": "^6.1.0",
    "date-fns": "^4.1.0",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jsondiffpatch": "^0.6.0",
    "lucide-react": "^0.428.0",
    "react-day-picker": "^9.5.0",
    "react-hot-toast": "^2.5.1",
    "react-i18next": "^15.7.3",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.6.0",
    "tinycolor2": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}
