{
  "name": "@alga-psa/email",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./system/*": {
      "types": "./dist/system/*.d.ts",
      "import": "./dist/system/*.js",
      "require": "./dist/system/*.cjs"
    },
    "./providers/*": {
      "types": "./dist/providers/*.d.ts",
      "import": "./dist/providers/*.js",
      "require": "./dist/providers/*.cjs"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "import": "./dist/lib/*.js",
      "require": "./dist/lib/*.cjs"
    },
    "./tenant/*": {
      "types": "./dist/tenant/*.d.ts",
      "import": "./dist/tenant/*.js",
      "require": "./dist/tenant/*.cjs"
    },
    "./actions": {
      "types": "./src/actions/index.ts",
      "import": "./src/actions/index.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "@alga-psa/core": "*",
    "@alga-psa/db": "*",
    "@alga-psa/types": "*",
    "nodemailer": "^6.9.14"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.7.3"
  }
}
