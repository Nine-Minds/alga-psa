{
  "name": "@alga-psa/projects",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./models": {
      "types": "./dist/models/index.d.ts",
      "import": "./dist/models/index.js",
      "require": "./dist/models/index.cjs"
    },
    "./models/*": {
      "types": "./dist/models/*.d.ts",
      "import": "./dist/models/*.js",
      "require": "./dist/models/*.cjs"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "import": "./dist/lib/*.js",
      "require": "./dist/lib/*.cjs"
    },
    "./schemas/*": {
      "types": "./dist/schemas/*.d.ts",
      "import": "./dist/schemas/*.js",
      "require": "./dist/schemas/*.cjs"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.js",
      "require": "./dist/types/*.cjs"
    },
    "./actions": {
      "types": "./src/actions/index.ts",
      "import": "./src/actions/index.ts"
    },
    "./actions/*": {
      "types": "./src/actions/*.ts",
      "import": "./src/actions/*.ts"
    },
    "./actions/projectActions": {
      "types": "./src/actions/projectActions.ts",
      "import": "./src/actions/projectActions.ts"
    },
    "./actions/projectTaskActions": {
      "types": "./src/actions/projectTaskActions.ts",
      "import": "./src/actions/projectTaskActions.ts"
    },
    "./actions/projectTaskCommentActions": {
      "types": "./src/actions/projectTaskCommentActions.ts",
      "import": "./src/actions/projectTaskCommentActions.ts"
    },
    "./actions/projectTaskStatusActions": {
      "types": "./src/actions/projectTaskStatusActions.ts",
      "import": "./src/actions/projectTaskStatusActions.ts"
    },
    "./actions/projectTemplateActions": {
      "types": "./src/actions/projectTemplateActions.ts",
      "import": "./src/actions/projectTemplateActions.ts"
    },
    "./actions/projectTemplateWizardActions": {
      "types": "./src/actions/projectTemplateWizardActions.ts",
      "import": "./src/actions/projectTemplateWizardActions.ts"
    },
    "./actions/phaseTaskImportActions": {
      "types": "./src/actions/phaseTaskImportActions.ts",
      "import": "./src/actions/phaseTaskImportActions.ts"
    },
    "./actions/regenerateOrderKeys": {
      "types": "./src/actions/regenerateOrderKeys.ts",
      "import": "./src/actions/regenerateOrderKeys.ts"
    },
    "./actions/serviceCatalogActions": {
      "types": "./src/actions/serviceCatalogActions.ts",
      "import": "./src/actions/serviceCatalogActions.ts"
    },
    "./lib/projectUtils": {
      "types": "./src/lib/projectUtils.ts",
      "import": "./src/lib/projectUtils.ts"
    },
    "./lib/orderingService": {
      "types": "./src/lib/orderingService.ts",
      "import": "./src/lib/orderingService.ts"
    },
    "./components": {
      "types": "./src/components/index.ts",
      "import": "./src/components/index.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@alga-psa/core": "*",
    "@alga-psa/db": "*",
    "@alga-psa/types": "*",
    "@alga-psa/validation": "*",
    "@alga-psa/clients": "*",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "tsup": "^8.0.0"
  }
}
