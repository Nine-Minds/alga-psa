{
  "name": "@alga-psa/tickets",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./models/*": {
      "types": "./dist/models/*.d.ts",
      "import": "./dist/models/*.js",
      "require": "./dist/models/*.cjs"
    },
    "./schemas/*": {
      "types": "./dist/schemas/*.d.ts",
      "import": "./dist/schemas/*.js",
      "require": "./dist/schemas/*.cjs"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "import": "./dist/lib/*.js",
      "require": "./dist/lib/*.cjs"
    },
    "./services/*": {
      "types": "./dist/services/*.d.ts",
      "import": "./dist/services/*.js",
      "require": "./dist/services/*.cjs"
    },
    "./lib": {
      "types": "./src/lib/index.ts",
      "import": "./src/lib/index.ts"
    },
    "./actions": {
      "types": "./src/actions/index.ts",
      "import": "./src/actions/index.ts"
    },
    "./actions/*": {
      "types": "./src/actions/*.ts",
      "import": "./src/actions/*.ts"
    },
    "./actions/board-actions": {
      "types": "./src/actions/board-actions/index.ts",
      "import": "./src/actions/board-actions/index.ts"
    },
    "./actions/board-actions/boardActions": {
      "types": "./src/actions/board-actions/boardActions.ts",
      "import": "./src/actions/board-actions/boardActions.ts"
    },
    "./actions/comment-actions": {
      "types": "./src/actions/comment-actions/index.ts",
      "import": "./src/actions/comment-actions/index.ts"
    },
    "./actions/comment-actions/commentActions": {
      "types": "./src/actions/comment-actions/commentActions.ts",
      "import": "./src/actions/comment-actions/commentActions.ts"
    },
    "./actions/ticket-number-actions": {
      "types": "./src/actions/ticket-number-actions/index.ts",
      "import": "./src/actions/ticket-number-actions/index.ts"
    },
    "./actions/ticket-number-actions/ticketNumberActions": {
      "types": "./src/actions/ticket-number-actions/ticketNumberActions.ts",
      "import": "./src/actions/ticket-number-actions/ticketNumberActions.ts"
    },
    "./components": {
      "types": "./src/components/index.ts",
      "import": "./src/components/index.ts"
    },
    "./components/*": {
      "types": "./src/components/*.tsx",
      "import": "./src/components/*.tsx"
    },
    "./components/settings/*": {
      "types": "./src/components/settings/*.tsx",
      "import": "./src/components/settings/*.tsx"
    },
    "./components/ticket/*": {
      "types": "./src/components/ticket/*.tsx",
      "import": "./src/components/ticket/*.tsx"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@alga-psa/core": "*",
    "@alga-psa/db": "*",
    "@alga-psa/shared": "*",
    "@alga-psa/tags": "*",
    "@alga-psa/types": "*",
    "@alga-psa/ui": "*",
    "@alga-psa/validation": "*",
    "date-fns": "^3.6.0",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "tsup": "^8.0.0"
  }
}
