{
  "name": "@alga-psa/integrations",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./actions": {
      "types": "./dist/actions/index.d.ts",
      "import": "./dist/actions/index.mjs",
      "require": "./dist/actions/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    },
    "./email": {
      "types": "./dist/email/index.d.ts",
      "import": "./dist/email/index.mjs",
      "require": "./dist/email/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@alga-psa/core": "file:../core",
    "@alga-psa/db": "file:../db",
    "@alga-psa/types": "file:../types",
    "@alga-psa/validation": "file:../validation",
    "axios": "^1.7.4",
    "knex": "^3.1.0",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
