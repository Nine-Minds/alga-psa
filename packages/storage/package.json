{
  "name": "@alga-psa/storage",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./StorageService": {
      "import": "./src/StorageService.ts",
      "types": "./src/StorageService.ts"
    },
    "./types/storage": {
      "import": "./src/types/storage.ts",
      "types": "./src/types/storage.ts"
    },
    "./providers/StorageProvider": {
      "import": "./src/providers/StorageProvider.ts",
      "types": "./src/providers/StorageProvider.ts"
    },
    "./config/storage": {
      "import": "./src/config/storage.ts",
      "types": "./src/config/storage.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@alga-psa/auth": "*",
    "@alga-psa/core": "*",
    "@alga-psa/db": "*",
    "@alga-psa/event-bus": "*",
    "@alga-psa/shared": "*",
    "@alga-psa/validation": "*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}
