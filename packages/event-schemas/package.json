{
  "name": "@alga-psa/event-schemas",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./schemas": {
      "types": "./src/schemas/index.ts",
      "import": "./dist/schemas/index.mjs",
      "require": "./dist/schemas/index.js"
    },
    "./schemas/*": {
      "types": "./src/schemas/*.ts",
      "import": "./dist/schemas/*.mjs",
      "require": "./dist/schemas/*.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.7.3"
  }
}
