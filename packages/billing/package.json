{
  "name": "@alga-psa/billing",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./constants/billing": {
      "types": "./dist/constants/billing.d.ts",
      "import": "./dist/constants/billing.js",
      "require": "./dist/constants/billing.cjs"
    },
    "./models": {
      "types": "./dist/models/index.d.ts",
      "import": "./dist/models/index.js",
      "require": "./dist/models/index.cjs"
    },
    "./models/*": {
      "types": "./dist/models/*.d.ts",
      "import": "./dist/models/*.js",
      "require": "./dist/models/*.cjs"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.js",
      "require": "./dist/schemas/index.cjs"
    },
    "./repositories/*": {
      "types": "./dist/repositories/*.d.ts",
      "import": "./dist/repositories/*.js",
      "require": "./dist/repositories/*.cjs"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.js",
      "require": "./dist/services/index.cjs"
    },
    "./services/*": {
      "types": "./dist/services/*.d.ts",
      "import": "./dist/services/*.js",
      "require": "./dist/services/*.cjs"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "import": "./dist/lib/*.js",
      "require": "./dist/lib/*.cjs"
    },
    "./lib/contractLineDisambiguation": {
      "types": "./src/lib/contractLineDisambiguation.ts",
      "import": "./src/lib/contractLineDisambiguation.ts"
    },
    "./actions": {
      "types": "./src/actions/index.ts",
      "import": "./src/actions/index.ts"
    },
    "./actions/*": {
      "types": "./src/actions/*.ts",
      "import": "./src/actions/*.ts"
    },
    "./components": {
      "types": "./src/components/index.ts",
      "import": "./src/components/index.ts"
    },
    "./components/*": {
      "types": "./src/components/*.tsx",
      "import": "./src/components/*.tsx"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@alga-psa/core": "*",
    "@alga-psa/db": "*",
    "@alga-psa/shared": "*",
    "@alga-psa/types": "*",
    "@alga-psa/validation": "*",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "tsup": "^8.0.0"
  }
}
