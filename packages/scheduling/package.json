{
  "name": "@alga-psa/scheduling",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./models": {
      "types": "./dist/models/index.d.ts",
      "import": "./dist/models/index.mjs",
      "require": "./dist/models/index.js"
    },
    "./models/*": {
      "types": "./dist/models/*.d.ts",
      "import": "./dist/models/*.mjs",
      "require": "./dist/models/*.js"
    },
    "./lib/contractLineDisambiguation": {
      "types": "./src/lib/contractLineDisambiguation.ts",
      "import": "./src/lib/contractLineDisambiguation.ts"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "import": "./dist/lib/*.mjs",
      "require": "./dist/lib/*.js"
    },
    "./schemas/*": {
      "types": "./dist/schemas/*.d.ts",
      "import": "./dist/schemas/*.mjs",
      "require": "./dist/schemas/*.js"
    },
    "./services/*": {
      "types": "./dist/services/*.d.ts",
      "import": "./dist/services/*.mjs",
      "require": "./dist/services/*.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.mjs",
      "require": "./dist/utils/*.js"
    },
    "./actions": {
      "types": "./src/actions/index.ts",
      "import": "./src/actions/index.ts"
    },
    "./actions/appointmentHelpers": {
      "types": "./src/actions/appointmentHelpers.ts",
      "import": "./src/actions/appointmentHelpers.ts"
    },
    "./actions/appointmentRequestManagementActions": {
      "types": "./src/actions/appointmentRequestManagementActions.ts",
      "import": "./src/actions/appointmentRequestManagementActions.ts"
    },
    "./actions/availabilitySettingsActions": {
      "types": "./src/actions/availabilitySettingsActions.ts",
      "import": "./src/actions/availabilitySettingsActions.ts"
    },
    "./actions/scheduleActions": {
      "types": "./src/actions/scheduleActions.ts",
      "import": "./src/actions/scheduleActions.ts"
    },
    "./actions/serviceCatalogActions": {
      "types": "./src/actions/serviceCatalogActions.ts",
      "import": "./src/actions/serviceCatalogActions.ts"
    },
    "./actions/timeEntryCrudActions": {
      "types": "./src/actions/timeEntryCrudActions.ts",
      "import": "./src/actions/timeEntryCrudActions.ts"
    },
    "./actions/timeEntryActions": {
      "types": "./src/actions/timeEntryActions.ts",
      "import": "./src/actions/timeEntryActions.ts"
    },
    "./actions/timeEntryHelpers": {
      "types": "./src/actions/timeEntryHelpers.ts",
      "import": "./src/actions/timeEntryHelpers.ts"
    },
    "./actions/timeEntrySchemas": {
      "types": "./src/actions/timeEntrySchemas.ts",
      "import": "./src/actions/timeEntrySchemas.ts"
    },
    "./actions/timeEntryServices": {
      "types": "./src/actions/timeEntryServices.ts",
      "import": "./src/actions/timeEntryServices.ts"
    },
    "./actions/timeEntryWorkItemActions": {
      "types": "./src/actions/timeEntryWorkItemActions.ts",
      "import": "./src/actions/timeEntryWorkItemActions.ts"
    },
    "./actions/timePeriodsActions": {
      "types": "./src/actions/timePeriodsActions.ts",
      "import": "./src/actions/timePeriodsActions.ts"
    },
    "./actions/timeSheetActions": {
      "types": "./src/actions/timeSheetActions.ts",
      "import": "./src/actions/timeSheetActions.ts"
    },
    "./actions/timeSheetOperations": {
      "types": "./src/actions/timeSheetOperations.ts",
      "import": "./src/actions/timeSheetOperations.ts"
    },
    "./actions/workItemActions": {
      "types": "./src/actions/workItemActions.ts",
      "import": "./src/actions/workItemActions.ts"
    },
    "./actions/time-period-settings-actions": {
      "types": "./src/actions/time-period-settings-actions/index.ts",
      "import": "./src/actions/time-period-settings-actions/index.ts"
    },
    "./actions/time-period-settings-actions/timePeriodSettingsActions": {
      "types": "./src/actions/time-period-settings-actions/timePeriodSettingsActions.ts",
      "import": "./src/actions/time-period-settings-actions/timePeriodSettingsActions.ts"
    },
    "./actions/*": {
      "types": "./src/actions/*.ts",
      "import": "./src/actions/*.ts"
    },
    "./components": {
      "types": "./src/components/index.ts",
      "import": "./src/components/index.ts"
    },
    "./components/*": {
      "types": "./src/components/*.tsx",
      "import": "./src/components/*.tsx"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@alga-psa/core": "*",
    "@alga-psa/db": "*",
    "@alga-psa/types": "*",
    "@alga-psa/validation": "*",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "tsup": "^8.0.0"
  }
}
