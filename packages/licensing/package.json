{
  "name": "@alga-psa/licensing",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./lib": {
      "types": "./dist/lib/get-license-usage.d.ts",
      "import": "./dist/lib/get-license-usage.js",
      "require": "./dist/lib/get-license-usage.cjs"
    },
    "./actions": {
      "types": "./src/actions/index.ts",
      "import": "./src/actions/index.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@alga-psa/core": "*",
    "@alga-psa/db": "*",
    "@alga-psa/tenancy": "*",
    "@alga-psa/types": "*",
    "@alga-psa/validation": "*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}
