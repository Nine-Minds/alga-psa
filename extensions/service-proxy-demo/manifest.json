{
  "name": "com.alga.sample.service-proxy",
  "publisher": "Alga",
  "version": "0.1.8",
  "runtime": "wasm-js@1",
  "capabilities": [
    "cap:context.read",
    "cap:log.emit",
    "cap:secrets.get",
    "cap:http.fetch",
    "cap:ui.proxy"
  ],
  "ui": {
    "type": "iframe",
    "entry": "ui/index.html",
    "hooks": {
      "appMenu": { "label": "Service Proxy Demo" }
    }
  },
  "assets": ["ui/**/*"]
}
